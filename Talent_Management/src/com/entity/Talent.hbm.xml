<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.entity.Talent" table="talent" schema="talent_management">
        <id name="talentId" column="TalentID"/>
        <property name="password" column="Password"/>
        <property name="name" column="Name"/>
        <property name="sex" column="Sex"/>
        <property name="idNumber" column="IDNumber"/>
        <property name="birthday" column="Birthday"/>
        <property name="phoneNumber" column="PhoneNumber"/>
        <property name="address" column="Address"/>
        <property name="education" column="Education"/>
        <property name="personalProfile" column="PersonalProfile"/>
        <property name="informationReview" column="InformationReview"/>
        <property name="identity" column="Identity"/>
    </class>
</hibernate-mapping>