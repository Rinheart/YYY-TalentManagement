<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.entity.Applicate" table="applicate" schema="talent_management">
        <id name="applicateId" column="ApplicateID"/>
        <property name="talentId" column="TalentID"/>
        <property name="applicateType" column="ApplicateType"/>
        <property name="enterpriseId" column="EnterpriseID"/>
        <property name="departmentId" column="DepartmentID"/>
        <property name="applicateTime" column="ApplicateTime"/>
        <property name="applicateResult" column="ApplicateResult"/>
        <property name="hrReview" column="HRReview"/>
        <property name="reviewTime" column="ReviewTime"/>
    </class>
</hibernate-mapping>