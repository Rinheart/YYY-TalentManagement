<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.entity.v_Enterprise_Talent"
           table="v_enterprise_talent"
           schema="talent_management">
        <id name="talentId" column="TalentID"/>
        <property name="name" column="Name"/>
        <property name="enterpriseId" column="EnterpriseID"/>
        <property name="identity" column="Identity"/>
    </class>
</hibernate-mapping>