<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.entity.v_WorkExperience" table="v_work_experience" schema="talent_management">
        <id name="workExperienceId" column="WorkExperienceID"/>
        <property name="talentId" column="TalentID"/>
        <property name="name" column="Name"/>
        <property name="enterpriseId" column="EnterpriseID"/>
        <property name="enterpriseName" column="EnterpriseName"/>
        <property name="departmentId" column="DepartmentID"/>
        <property name="departmentName" column="DepartmentName"/>
        <property name="startTime" column="StartTime"/>
        <property name="endTime" column="EndTime"/>
    </class>
</hibernate-mapping>